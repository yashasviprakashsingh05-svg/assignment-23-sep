import java.util.Scanner;

public class DrumBeats {
    public static void printBeats(int n) {
        if (n == 0) {
            return; // base case
        }
        printBeats(n - 1);  // recursive call for numbers before n
        System.out.print(n + " "); // print n after recursion
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        printBeats(n);
    }
}
